{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "description": "A simple bar chart with embedded data.",
  "title": "Mean years of Schooling",
  "data": {
    "url": "https://raw.githubusercontent.com/allanrono/FIT3179_Assignment_2_Resubmission/main/HDI.csv"
  },
  "mark": "bar",
  "color": "green",
  "encoding": {
    "x": {"field": "Country", "type": "ordinal"},
    "y": {"field": "Mean years of schooling", "type": "quantitative"},
    "color": {"value": "darkgreen"},
    "tooltip": [
      {"field": "Mean years of schooling Female", "type": "quantitative"},
      {"field": "Mean years of schooling Male", "type": "quantitative"}
    ]
  },
  "config": {}
}